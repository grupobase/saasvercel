{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.groupbasebytecn.com.br" }],
      "destination": "https://groupbasebytecn.com.br/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "header", "key": "x-forwarded-proto", "value": "http" }],
      "destination": "https://groupbasebytecn.com.br/:path*",
      "permanent": true
    }
  ]
}
